<+>/* Styles CSS */\r\nbody {\r\n    font-family: Arial, sans-serif;\r\n    margin: 0;\r\n    padding: 0;\r\n}\r\n\r\n#promo-banner {\r\n    background-color: #000000;\r\n    padding: 10px;\r\n    text-align: center;\r\n    color: #FFFFFF;\r\n}\r\n\r\n#promo-banner a {\r\n    color: #FFFFFF;\r\n    text-decoration: none;\r\n}\r\n\r\n#navbar {\r\n    border: 3px solid black;\r\n    background-color: #FFFFFF;\r\n    padding: 10px;\r\n    display: flex;\r\n    align-items: center;\r\n}\r\n\r\n#navbar img {\r\n    margin-right: 10px;\r\n}\r\n\r\n#search-bar {\r\n    margin-left: 100px;\r\n    padding: 10px;\r\n    padding-right: 90px;\r\n    border: 1px solid #000000;\r\n    border-radius: 10px;\r\n}\r\n.search-icon {\r\n    background-image: url(\"../image/search.png\");\r\n    background-repeat: no-repeat;\r\n    background-size: 16px 16px;\r\n    display: inline-block;\r\n    width: 16px;\r\n    height: 16px;\r\n    margin-right: 200px;\r\n    margin-left: 5px;\r\n}\r\n\r\n#navbar a {\r\n    margin-right: 70px;\r\n    text-decoration: none;\r\n    color: #000000;\r\n}\r\n\r\n#main-content {\r\n    margin: 20px;\r\n}\r\n\r\n.rounded-box {\r\n    display: flex;\r\n    border-radius: 10px;\r\n    padding: 20px;\r\n    margin-bottom: 20px;\r\n    background-image: linear-gradient(210deg, rgba(235, 86, 123, 1) 0%, rgba(150, 158, 255, 0.5899999737739563) 48%, rgba(145, 58, 254, 0.8399999737739563) 100%);\r\n}\r\n\r\n.refurbished-left {\r\n    width: 50%;\r\n}\r\n\r\n.refurbished-right{\r\n    width: 50%;\r\n    display: flex;\r\n}\r\n.carousel-images img {\r\n    width: 70%;\r\n    height: 70%;\r\n    display: none;\r\n}\r\n\r\n.prev-button-item,\r\n.next-button-item{\r\n    padding-top: 180px;\r\n    width: 10%;\r\n}\r\n\r\n.prev-button,\r\n.next-button {\r\n    padding-top: 90px;\r\n    width: 10%;\r\n}\r\n.prev-button-item img,\r\n.next-button-item img{\r\n    width: 24px;\r\n    height: 24px;\r\n}\r\n\r\n.prev-button img,\r\n.next-button img{\r\n    width: 24px;\r\n    height: 24px;\r\n}\r\n.bg-arrow{\r\n    border: none;\r\n    background: none;\r\n    cursor: pointer;\r\n}\r\n.shop-refurbished{\r\n    width: 30%;\r\n    height: 40px;\r\n    border: 1px solid #000000;\r\n    border-radius: 10px;\r\n    display: flex;\r\n    background-color: #FFFFFF;\r\n    margin-left: 30%;\r\n}\r\n\r\n.shop-refurbished a{\r\n    padding: 8px;\r\n    text-decoration: none;\r\n    color: #000000;\r\n}\r\n.shop-refurbished img{\r\n    padding-top: 6px;\r\n}\r\n\r\n\r\n.product-description {\r\n    font-size: 10px;\r\n    color: #4A4848 ;\r\n}\r\n.title-1{\r\n    font-size: 30px;\r\n    color: #000000;\r\n\r\n}\r\n.title-2{\r\n    font-size: 24px;\r\n    color: #A8A8A8;\r\n}\r\n.Most-Wanted{\r\n    padding: 10px;\r\n    display: flex;\r\n    border-radius: 10px;\r\n    background-color: #ffffff;\r\n    gap: 25px;\r\n}\r\n\r\n.product-box {\r\n    border: 2px solid black;\r\n    position: relative;\r\n    border-radius: 10px;\r\n    background-color: #ffffff;\r\n}\r\n.best-seller{\r\n    width: 50%;\r\n    margin: 10px;\r\n    border-radius: 10px;\r\n    background-color: #DFDFDF;\r\n\r\n}\r\n.item-image{\r\n    margin-bottom: -35px;\r\n    margin-top: -20px;\r\n}\r\n.description{\r\n    margin-left:8px ;\r\n}\r\n\r\n.product-box img {\r\n    width: 200px;\r\n    height: 200px;\r\n    object-fit: cover;\r\n}\r\n\r\nfooter a {\r\n    color: #000000;\r\n    text-decoration: none;\r\n    margin: 0 10px;\r\n}\r\n
